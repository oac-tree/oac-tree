#  SEQUENCER_INCLUDE_DIRS - include directories
#  SEQUENCER_LIBRARIES    - libraries to link against

get_filename_component(SEQUENCER_CMAKE_DIR "${CMAKE_CURRENT_LIST_FILE}" PATH)

include(CMakeFindDependencyMacro)

find_dependency(sup-dto)
find_dependency(sup-utils)

if(TARGET sequencer::sequencer)
else()
  message(STATUS "sequencer-config.cmake including ${SEQUENCER_CMAKE_DIR}/sequencer-targets.cmake")
 include("${SEQUENCER_CMAKE_DIR}/sequencer-targets.cmake")
endif()

set(SEQUENCER_LIBRARIES sequencer::sequencer)

message(STATUS "sequencer-config.cmake in ${CMAKE_CURRENT_LIST_FILE}")
message(STATUS "SEQUENCER_LIBRARIES ${SEQUENCER_LIBRARIES}")
