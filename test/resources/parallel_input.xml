<?xml version="1.0" encoding="UTF-8"?>
<Procedure xmlns="http://codac.iter.org/sup/oac-tree" version="1.0"
           name="Procedure for testing parallel sequences"
           xmlns:xs="http://www.w3.org/2001/XMLSchema-instance"
           xs:schemaLocation="http://codac.iter.org/sup/oac-tree oac-tree.xsd"
           tickTimeout="0.2">
    <ParallelSequence name="Parallel Wait" successThreshold="2">
        <Input outputVar="number" description="Some number" />
        <Input outputVar="name" description="Name" />
    </ParallelSequence>
    <Workspace>
        <Local name="number" type='{"type":"int32"}'/>
        <Local name="name" type='{"type":"string"}' />
    </Workspace>
</Procedure>
